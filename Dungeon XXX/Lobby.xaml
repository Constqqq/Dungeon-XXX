<Window x:Name="lobbywin" x:Class="Dungeon_XXX.Lobby"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dungeon_XXX"
        mc:Ignorable="d"
        Title="Lobby" Height="1080" Width="1920 ">
    <Grid x:Name="gridlobby">
        <Canvas x:Name="LobbyCan" Focusable="True" KeyUp="KeyBoardUp" KeyDown="KeyboardDown">
            
                <Image Source="lobbyback.png" Stretch="Fill" Height="1050" Canvas.Left="-6" Width="1920"/>
            <Rectangle Canvas.Left="50" Canvas.Top="50" Width="100" Height="100" Fill="Black" x:Name="Player"></Rectangle>
        </Canvas>
    </Grid>
</Window>
