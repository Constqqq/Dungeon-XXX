<Window x:Name="lobbywin" x:Class="Dungeon_XXX.Lobby"
        
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dungeon_XXX"
        mc:Ignorable="d"
        Title="Lobby" Height="890.4" Width="1576.8">
    <Grid x:Name="gridlobby">
        <TextBlock x:Name="AmmoInfo" Text="Ammo: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <TextBlock x:Name="MagazineInfo" Text="Magazine: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,30,0,0"/>
        <Canvas x:Name="LobbyCan" Focusable="True" KeyUp="KeyBoardUp" KeyDown="KeyboardDown" Height="861" Width="1570"  VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,0,0,-0.2"  >



            <Rectangle  Canvas.Left="50" Canvas.Top="155" Width="55" Height="50" Fill="Black" Tag="User" x:Name="Player"></Rectangle>
            
            <Rectangle  Canvas.Left="643" Canvas.Top="140" Width="99" Height="208" Fill="Black"  Tag="Collide" RenderTransformOrigin="0.932,0.777"/>

        </Canvas>



    </Grid>
</Window>
